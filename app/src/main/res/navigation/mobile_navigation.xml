<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_history">

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.renata.view.fragment.history.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" >
    </fragment>
    <fragment
        android:id="@+id/navigation_scan"
        android:name="com.renata.view.fragment.scan.ScanFragment"
        android:label="fragment_scan"
        tools:layout="@layout/fragment_scan" >
    </fragment>
    <fragment
        android:id="@+id/navigation_account"
        android:name="com.renata.view.fragment.account.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
    </fragment>
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.renata.view.fragment.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
    </fragment>
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.renata.view.fragment.setting.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
    </fragment>
</navigation>